{
    "type": "project",
    "license": "MIT",
    "name": "heahdude/yaml-linter",
    "description": "A YAML linter Symfony single-command application",
    "keywords": ["yaml", "linter", "cli", "command", "symfony-command"],
    "require": {
        "php": "^7.1.3 || ^8.0.2",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Heah\\YamlLinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heah\\YamlLinter\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/yaml-linter"]
}
